<%= form_for @user do |f| %>
  <%= f.label :f_name %>
  <%= f.text_field :f_name %>
  <%= f.label :l_name %>
  <%= f.text_field :l_name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit %>
<% end %>
